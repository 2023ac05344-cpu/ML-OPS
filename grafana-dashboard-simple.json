{
    "dashboard": {
        "id": null,
        "title": "MLOps API Monitoring - Simple",
        "tags": [
            "mlops",
            "api",
            "monitoring"
        ],
        "style": "dark",
        "timezone": "browser",
        "panels": [
            {
                "id": 1,
                "title": "Total Predictions",
                "type": "stat",
                "targets": [
                    {
                        "expr": "predictions_total",
                        "refId": "A"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "color": {
                            "mode": "palette-classic"
                        },
                        "custom": {
                            "displayMode": "basic"
                        },
                        "mappings": [],
                        "thresholds": {
                            "mode": "absolute",
                            "steps": [
                                {
                                    "color": "green",
                                    "value": null
                                }
                            ]
                        }
                    }
                },
                "gridPos": {
                    "h": 8,
                    "w": 6,
                    "x": 0,
                    "y": 0
                }
            },
            {
                "id": 2,
                "title": "Prediction Rate (5m)",
                "type": "stat",
                "targets": [
                    {
                        "expr": "rate(predictions_total[5m])",
                        "refId": "A"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "color": {
                            "mode": "palette-classic"
                        },
                        "custom": {
                            "displayMode": "basic"
                        },
                        "mappings": [],
                        "thresholds": {
                            "mode": "absolute",
                            "steps": [
                                {
                                    "color": "green",
                                    "value": null
                                }
                            ]
                        },
                        "unit": "reqps"
                    }
                },
                "gridPos": {
                    "h": 8,
                    "w": 6,
                    "x": 6,
                    "y": 0
                }
            },
            {
                "id": 3,
                "title": "Total CPU Time (seconds)",
                "type": "stat",
                "targets": [
                    {
                        "expr": "process_cpu_seconds_total",
                        "refId": "A"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "color": {
                            "mode": "palette-classic"
                        },
                        "custom": {
                            "displayMode": "basic"
                        },
                        "mappings": [],
                        "thresholds": {
                            "mode": "absolute",
                            "steps": [
                                {
                                    "color": "green",
                                    "value": null
                                }
                            ]
                        },
                        "unit": "s"
                    }
                },
                "gridPos": {
                    "h": 8,
                    "w": 6,
                    "x": 12,
                    "y": 0
                }
            },
            {
                "id": 4,
                "title": "Memory Usage (MB)",
                "type": "stat",
                "targets": [
                    {
                        "expr": "process_resident_memory_bytes / 1024 / 1024",
                        "refId": "A"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "color": {
                            "mode": "palette-classic"
                        },
                        "custom": {
                            "displayMode": "basic"
                        },
                        "mappings": [],
                        "thresholds": {
                            "mode": "absolute",
                            "steps": [
                                {
                                    "color": "green",
                                    "value": null
                                }
                            ]
                        },
                        "unit": "MB"
                    }
                },
                "gridPos": {
                    "h": 8,
                    "w": 6,
                    "x": 18,
                    "y": 0
                }
            },
            {
                "id": 5,
                "title": "Average Prediction Duration (ms)",
                "type": "stat",
                "targets": [
                    {
                        "expr": "(prediction_duration_seconds_sum / prediction_duration_seconds_count) * 1000",
                        "refId": "A"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "color": {
                            "mode": "palette-classic"
                        },
                        "custom": {
