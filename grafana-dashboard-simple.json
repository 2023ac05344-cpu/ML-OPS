                    "h": 8,
                    "w": 6,
                    "x": 0,
                    "y": 8
                }
            },
            {
                "id": 6,
                "title": "Total Prediction Duration (ms)",
                "type": "stat",
                "targets": [
                    {
                        "expr": "prediction_duration_seconds_sum * 1000",
                        "refId": "A"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "color": {
